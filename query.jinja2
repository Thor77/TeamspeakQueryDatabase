<html>
<head>
  <meta charset="utf-8">
  <title>TeamspeakQueryDatabase</title>

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/pure/0.6.0/pure-min.css" integrity="sha256-AOg6pfZnxO4hlBH2z2ummgdGKU8XU9ugUPkyWaSMJ0c=" crossorigin="anonymous">
</head>
<body>
  <table class="pure-table">
    <thead><tr>
      <td>command</td>
      {% if 'arguments' in columns %}<td>arguments</td>{% endif %}
      {% if 'description' in columns %}<td>description</td>{% endif %}
      {% if 'permissions' in columns %}<td>permissions</td>{% endif %}
    </tr></thead>
    <tbody>
      {% for command, information in commands %}
      <tr>
        <td id="{{ command }}"><a href="#{{ command }}">{{ command }}</a></td>
        {% if 'arguments' in columns %}<td>{{ information.arguments }}</td>{% endif %}
        {% if 'description' in columns %}<td>{{ information.description }}</td>{% endif %}
        {% if 'permissions' in columns %}<td>{{ information.permissions }}</td>{% endif %}
      </tr>
      {% endfor %}
    </tbody>
  </table>
</body>
</html>
